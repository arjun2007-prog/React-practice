//Day 2

//Part 1
//Learnt how to style react element with css and how to add attributes to JSX html elements

import React from "react";
import ReactDOM from "react-dom";
import "../public/styles.css";

const imgLink = "https://picsum.photos/200/200" 
ReactDOM.render(
<div>
<h1 className="heading" contentEditable="true" spellCheck="false">Hello World!</h1>
//While using attributes in JSX elements you have to use camel casing if there are multiple words in the attribute
<div className="images">
 <img alt="Recat-js" className="framework-image" src="data:image/png;base64,"/>
 <img alt="node-js" className="framework-image" src="data:image/png;base64,"/>
 <img alt="express-js" className="framework-image" src="data:image/png;base64,"/>
 <img alt="random-image" src={imgLink}/>
</div>
</div>,
document.getElementById("root")
);

//Part 2

//Inline styling for JSX elements

import React from "react"
import ReactDOM from "react-dom"

const customStyle = 
{
  color:"red",
  fontSize:"200px"
}

ReactDOM.render(
//Here we need to put the css in objects and then put it in the style attribute
<h1 style={customStyle}>Hello</h1>,
document.getElementById("root")
);
